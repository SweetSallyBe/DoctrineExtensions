{
  "name": "sweet-sally-be/doctrine-extensions",
  "description": "Doctrine Extensions",
  "type": "symfony-bundle",
  "keywords": [
    "extensions","doctrine", "translatable"
  ],
  "homepage": "https://www.sweetsally.be",
  "license": "unlicensed",
  "authors": [
    { "name": "Sweet Sally", "homepage": "https://www.sweetsally.be" }
  ],
  "require": {
    "php": "^8.1",
    "doctrine/common": "^2.7|^3.0",
    "doctrine/persistence": "^2.0",
    "doctrine/dbal": "^2.9",
    "doctrine/orm": "^2.7",
    "symfony/framework-bundle": "^5.4"
  },
  "require-dev": {
    "psr/log": "^1.1",
    "doctrine/annotations": "^1.8",
    "doctrine/doctrine-bundle": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "SweetSallyBe\\DoctrineExtensions\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SweetSallyBe\\DoctrineExtensions\\Tests\\": "tests"
    }
  },
  "scripts": {
  }
}